{
  "name": "docuflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "docuflow": "./dist/cli/index.js"
  },
  "main": "./dist/cli/index.js",
  "scripts": {
    "dev": "vite ./src/website",
    "build": "tsc -b src/cli --verbose && tsc-alias -p ./src/tsconfig.json --verbose --resolve-full-paths",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview ./website"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/cli-plugin-typescript": "5.0.8",
    "@vue/cli-service": "^5.0.8",
    "autoprefixer": "^10.4.19",
    "commander": "^12.1.0",
    "eslint": "9.8.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "glob": "^11.0.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vite-ssg": "^0.23.8"
  }
}
